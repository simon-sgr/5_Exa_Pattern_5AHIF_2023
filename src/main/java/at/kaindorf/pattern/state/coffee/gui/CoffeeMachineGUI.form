<?xml version="1.0" encoding="UTF-8"?>
<form xmlns="http://www.intellij.com/uidesigner/form/" version="1" bind-to-class="at.kaindorf.pattern.state.coffee.gui.CoffeeMachineGUI">
  <grid id="27dc6" binding="pnlContent" layout-manager="BorderLayout" hgap="0" vgap="0">
    <constraints>
      <xy x="20" y="20" width="500" height="400"/>
    </constraints>
    <properties/>
    <border type="none"/>
    <children>
      <component id="c8ef8" class="javax.swing.JLabel" binding="lblCurrentStatus">
        <constraints border-constraint="South"/>
        <properties>
          <font size="26"/>
          <horizontalAlignment value="0"/>
          <text value="Current Status: "/>
        </properties>
      </component>
      <grid id="e4815" binding="pnlButtons" layout-manager="GridLayoutManager" row-count="3" column-count="2" same-size-horizontally="false" same-size-vertically="false" hgap="-1" vgap="-1">
        <margin top="10" left="10" bottom="10" right="10"/>
        <constraints border-constraint="Center"/>
        <properties/>
        <border type="none"/>
        <children>
          <component id="9b02c" class="javax.swing.JButton" binding="btnInsertCoins">
            <constraints>
              <grid row="0" column="0" row-span="1" col-span="1" vsize-policy="0" hsize-policy="3" anchor="0" fill="1" indent="0" use-parent-layout="false"/>
            </constraints>
            <properties>
              <font size="26"/>
              <text value="Insert Coins"/>
            </properties>
          </component>
          <component id="31e0f" class="javax.swing.JButton" binding="btnRemoveCoins">
            <constraints>
              <grid row="0" column="1" row-span="1" col-span="1" vsize-policy="0" hsize-policy="3" anchor="0" fill="1" indent="0" use-parent-layout="false"/>
            </constraints>
            <properties>
              <font size="26"/>
              <text value="Remove Coins"/>
            </properties>
          </component>
          <component id="6723d" class="javax.swing.JButton" binding="btnEspresso">
            <constraints>
              <grid row="1" column="0" row-span="1" col-span="1" vsize-policy="0" hsize-policy="3" anchor="0" fill="1" indent="0" use-parent-layout="false"/>
            </constraints>
            <properties>
              <font size="26"/>
              <text value="Espresso"/>
            </properties>
          </component>
          <component id="52a0" class="javax.swing.JButton" binding="btnCappucinno">
            <constraints>
              <grid row="1" column="1" row-span="1" col-span="1" vsize-policy="0" hsize-policy="3" anchor="0" fill="1" indent="0" use-parent-layout="false"/>
            </constraints>
            <properties>
              <font size="26"/>
              <text value="Cappucinno"/>
            </properties>
          </component>
          <component id="2d871" class="javax.swing.JButton" binding="btnFinishCoffee">
            <constraints>
              <grid row="2" column="0" row-span="1" col-span="1" vsize-policy="0" hsize-policy="3" anchor="0" fill="1" indent="0" use-parent-layout="false"/>
            </constraints>
            <properties>
              <font size="26"/>
              <text value="Finish Coffee"/>
            </properties>
          </component>
          <component id="69977" class="javax.swing.JButton" binding="btnAddCaffeine">
            <constraints>
              <grid row="2" column="1" row-span="1" col-span="1" vsize-policy="0" hsize-policy="3" anchor="0" fill="1" indent="0" use-parent-layout="false"/>
            </constraints>
            <properties>
              <font size="26"/>
              <text value="Add Caffeine"/>
            </properties>
          </component>
        </children>
      </grid>
      <component id="f1dd1" class="javax.swing.JLabel" binding="lblMoneyCaffeine">
        <constraints border-constraint="North"/>
        <properties>
          <font size="26"/>
          <horizontalAlignment value="0"/>
          <text value="Mony and Caffeine Amount: "/>
        </properties>
      </component>
    </children>
  </grid>
</form>
